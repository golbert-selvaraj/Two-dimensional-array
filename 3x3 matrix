#include <stdio.h>
int main () 
{ int arr[3][3],a=0, b=0;
  for(a=0;a<3;a++) 
   {for(b=0;b<3;b++) 
     {printf("element-[%d][%d]: ", a, b);
      scanf("%d", &arr[a][b]) ;}}
      
printf("The matrix is :\n");
  for(a=0;a<3;a++) 
   {{for(b=0;b<3;b++) 
      printf("%d ", arr[a][b]);}
      printf("\n");}
}
